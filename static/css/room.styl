                    .actions {
                        padding: 20px 20px 0;
                        
                        a {
                            width: 110px;
                            height: 36px;
                            line-height: 36px;
                            text-align: center;
                            display: block;
                            float: left;
                            border: 1px solid #D4DAE0;
                            box-shadow: 0 2px 4px 0 rgba(226, 234, 239, 0.50);
                            border-radius: 4px;
                            font-size: 14px;
                            transition-duration: .3s;
                            position relative
                            
                            &:active {
                                top 1px
                            }
                            
                            &:not(:first-child) {
                                margin-left 20px
                            }
                            
                            &.clear:hover {
                                box-shadow: 0 2px 4px 0 rgba(176, 182, 185, 0.5);
                            }
                            
                            &.recommend {
                                color: #0C9DFC;
                                font-weight: bold;
                                border: 1px solid #0C9DFC;
                                
                                &:hover {
                                    box-shadow: 0 2px 4px 0 rgba(12, 157, 252, 0.53);
                                }
                            }
                            
                            &.saved {
                                color: #4BDCBA;
                                font-weight: bold;
                                border: 1px solid #4BDCBA;
                                position relative
                                
                                &.selected {
                                    background-color: #4BDCBA;
                                    color: #fff;
                                }
                                
                                &:hover {
                                    box-shadow: 0 2px 4px 0 rgba(75, 220, 186, 0.5);
                                }
                                
                                .el-badge {
                                    font-family: Helvetica Neue,Helvetica,PingFang SC,Hiragino Sans GB,Microsoft YaHei,SimSun,sans-serif;
                                    font-weight normal
                                    position absolute
                                    right -8px
                                    top -12px
                                }
                            }
                        }
                    }
                    
                    .info {
                        margin-top: 30px;
                        padding: 0 10px;
                        
                        .item {
                            display: block;
                            float: left;
                            width: calc(100% / 3);
                            text-align: center;
                            position: relative;
                            
                            .dialog {
                                left: -140px;
                                top: -130px;
                            }
                            
                            .key {
                                font-size: 14px;
                                height: 20px;
                                line-height: 20px;
                            }
                            
                            .val {
                                margin-bottom: 5px;
                                font-size: 24px;
                                height: 28px;
                                line-height: 30px;
                                font-family: TradeGothicLT-Bold;
                                
                                span {
                                    margin-left 3px
                                }
                            }
                        }
                    }
                    
                }
            }
        }
    }
    
    > .footer {
        position: fixed;
        bottom: 0;
        left: 0;
        right: 0;
        z-index: 100;
    
        .container {
            background-color: #fff;
            border: 1px solid #E7EDF1;
            box-shadow: 0 -1px 4px 0 rgba(226, 234, 239, 0.80);
            height: 80px;
            padding: 22px 30px;
    
            .join {
                height: 36px;
                line-height: 36px;
    
                .count {
                    font-family: TradeGothicLT-Bold;
                    font-size: 24px;
                    color: #0C9DFC;
                    margin-left: 10px;
                }
                
                > span {
                    vertical-align: middle;
                }
            }
            
            .submit-btn {
                user-select none
                cursor pointer
                background: #0C9DFC;
                border: 1px solid #0397F9;
                border-radius: 4px;
                height: 36px;
                line-height: 36px;
                display: inline-block;
                vertical-align: middle;
                font-size: 14px;
                color: #fff;
                font-weight: bold;
                padding: 0 25px;
                transition-duration .3s
        
                &:hover {
                    box-shadow: 0 2px 4px 0 rgba(12, 157, 252, 0.40);
                }
                
                &.disabled {
                    opacity: 0.3;
                    background: #0C9DFC;
                    border: 1px solid #0397F9;
                    border-radius: 4px;
                    cursor: no-drop;
                }
            }
        }
    }
    
}

