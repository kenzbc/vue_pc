                            }
                        }
                        
                        .close {
                            height: 26px;
                            width: 26px;
                            line-height: 24px;
                            text-align: center;
                            user-select none
                            cursor pointer
                            border-radius: 50%;
                            position: absolute;
                            right: 22px;
                            top: 22px;
                            display: block;
                            transition-duration: .3s;
                            color: #FF5C5C;
                            
                            .icon {
                                height: 26px;
                                width: 26px;
                            }
        
                            &:hover {
                                box-shadow: 0 2px 5px 0 rgba(255, 92, 92, 0.62);
                            }
                        }
                    }
                    
                    .group-list {
                        .scrollbar {
                            height: 740px;
                            padding-top 15px
                        }
                        
                        .group {
                            width: 750px;
                            //height: 320px;
