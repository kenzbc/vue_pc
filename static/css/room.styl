                            > * {
                                float: left;
                                height: 100px;
                            }
                            
                            .info {
                                width: 190px;
                                padding: 15px 10px;
                                font-family: TradeGothicLT-Bold;
                                
                                .player-name {
                                    font-size 22px
                                    line-height 26px
                                    height 26px
                                    cursor pointer
                                    span {
                                        display inline-block
                                        vertical-align middle
                                    }
    
                                    .badge {
                                        padding 0 3px
                                        height 18px
                                        line-height 18px
                                        border-radius 2px
                                        font-size 12px
                                        text-align center
                                        margin-left 5px
                                        color #fff
        
                                        &.status-1 {
                                            background-color #0C9DFC
                                        }
                                        
                                        &.status-2 {
                                            background-color #bebebe
                                        }
                                        
                                        &.status-4 {
                                            background-color #ffb30f
                                        }
                                    }
                                }
                                
                                .team-name, .vs-team {
                                    line-height: 18px;
                                    height: 18px;
                                }
                                
                                .team-name {
                                    font-size: 14px;
                                    color: #A5A8B2;
                                    margin: 2px 0 4px;
                                }
                                
                                .vs-team-list {
                                    &:before {
                                        content: '';
                                        display: block;
                                        clear: both;
                                    }
                                    
                                    .vs-team {
                                        background: #373A41;
                                        border-radius: 2px;
                                        padding: 0 5px;
                                        float: left;
                                        font-size: 12px;
                                        color: #FFFFFF;
                                        
                                        + .vs-team {
                                            margin-left: 5px;
                                        }
                                    }
                                }
                            }
                            
                            .avg-score {
                                width: 240px;
                                padding: 0 10px 10px;
                                text-align: center;
                                position relative
                                
                                .chart {
                                    > div {
                                        margin 0 auto
                                    }
                                }
                                
                                .score {
                                    font-size: 14px;
                                    position absolute
                                    bottom 15px
                                    width 100%
                                    text-align center
                                }
                                
                            }
                            
                            .price {
                                width: 110px;
                                text-align: center;
                                font-size: 24px;
                                line-height: 100px;
                                font-family: TradeGothicLT-Bold;
                                transition-duration .3s
                                
                                span {
                                    margin-left 2px
                                }
                            }
                            
                            .action {
                                width: 70px;
                                position: relative;
    
                                .dialog {
                                    left: -177px;
                                    top: -102px;
                                }
                                
                                .btn {
                                    font-family: 'icon';
                                    font-size: 32px;
                                    cursor pointer
                                    width: 32px;
                                    height: 100px;
                                    line-height 100px
                                    background-size: 32px;
                                    display: block;
                                    margin: 0 auto;
                                    position relative
                                    
