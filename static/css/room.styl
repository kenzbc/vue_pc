#room {
    .dialog {
        width: 270px;
        height: 118px;
        position: absolute;
        background: #fff;
        border: 2px solid #0C9DFC;
        border-radius: 2px;
        z-index: 9;
        box-shadow: 0 2px 4px 0 rgba(0, 0, 0, 0.12), 0 0 6px 0 rgba(0, 0, 0, 0.04);
        text-align: center;
        padding: 30px 15px;
        
        .close {
            position: absolute;
            right: 0;
            top: 0;
            padding: 10px;
            line-height: 10px;
            text-align: center;
            
            &:before {
                font-family: 'icon';
                color: #A5A8B2;
                content: '\e614';
                font-size: 10px;
                display: inline-block;
            }
        }
        
        .title {
            font-size: 16px;
            font-weight: bold;
        }
        
        .text {
            font-size: 14px;
            margin-top: 15px;
        }
        
        &:before {
            content: '';
            position: absolute;
            right: 43px;
            bottom: -15px;
            display: block;
            border-top: 15px solid #0C9DFC;
            border-left: 15px solid transparent;
            border-right: 15px solid transparent;
        }
        
        &:after {
            content: '';
            position: absolute;
            right: 46px;
            bottom: -12px;
            display: block;
            border-top: 12px solid #fff;
            border-left: 12px solid transparent;
            border-right: 12px solid transparent;
        }
    }
    
    > .info {
        /*background-image: linear-gradient(0deg, #191F2A 0%, #0C0920 79%);*/
        height: 140px;
        padding: 34px 0 24px 0;
        background-image: url(/static/image/room-info.jpg);
        background-size: cover;
        
        .container {
            color: #fff;
        }
        
    }
    
    > .main {
        margin-top: 20px;
        height: 100vh;
        
        .good {
            color #DABE9B
        }
        
        .red {
            color #ff252f
        }
        
        .blue {
            color #0C9DFC
        }
        
        .container {
            background: #fafbfd;
            box-shadow: 0 2px 4px 0 rgba(226, 234, 239, 0.80);
            border: 1px solid #E7EDF1;
            border-radius: 2px;
            height 100vh
            
            .left > .select-box > .title,
            .right > .title {
                font-size: 16px;
                font-weight: bold;
                height: 50px;
                line-height: 50px;
                padding: 0 20px;
            }
            
            .left {
                position relative
                width: 788px;
                border-right: 1px solid #E7EDF1;
                -webkit-perspective: 800px;
                -moz-perspective: 800px;
                overflow hidden
                
                > .select-box,.saved-box {
                    left: 0;
                    width: 100%;
                    height: 100%;
                    -webkit-transform-style: preserve-3d;
                    -moz-transform-style: preserve-3d;
                    -webkit-transition: .3s ease-in-out;
                    -moz-transition: .3s ease-in-out;
                    -webkit-backface-visibility: hidden;
                    -moz-backface-visibility: hidden;
    
                    &.select-box {
                        -webkit-transform: rotateY(0);
                        -moz-transform: rotateY(0);
                        z-index: 2;
                        opacity 1
                    }
                    
                    &.saved-box {
                        -webkit-transform: rotateY(180deg);
                        -moz-transform: rotateY(180deg);
                        z-index: 2;
                        opacity 0
                    }
                }
                
                &.saved {
                    > .select-box {
                        -webkit-transform: rotateY(-180deg);
                        -moz-transform: rotateY(-180deg);
                        opacity 0
                    }
                    > .saved-box {
                        -webkit-transform: rotateY(0);
                        -moz-transform: rotateY(0);
                        opacity 1
                    }
                }
                
                .pos-nav {
                    height: 50px;
                    border-bottom: 1px solid #E7EDF1;
                    box-shadow: 0 1px 4px 0 rgba(228, 228, 228, 0.87)
                    position: relative;
                    background-color: #fff;
                    z-index 10
                    
                    .pos-item {
                        user-select none
                        height: 50px;
                        line-height: 50px;
                        float: left;
                        width: calc(100% / 5);
                        border-bottom: 2px solid transparent;
                        text-align: center;
                        font-size: 14px;
                        font-weight: bold;
                        cursor: pointer;
                        background-color #fff
                        border-bottom 1px solid #E7EDF1
                        transition-duration .3s
                        
                        &:hover {
                            background-color #F3F5F7
                        }
                        
                        .icon {
                            width 24px
                            height 24px
                            
                            &.select {
                                color #b4b7c1
                                opacity .5
                                transition-duration .3s
                                width 16px
                                height 16px
                                margin-left 10px
                            }
                        }
                        
                        &.selected {
                            .icon.select {
                                color #0c9dfc
                                opacity 1
                            }
                        }
                        
                        &:not(:last-child) {
                            border-right: 1px solid #E7EDF1;
                        }
                        
                        &.active {
                            border-bottom: 2px solid #373A41;
                        }
                        
                        img {
                            height: 32px;
                            width: 32px;
                        }
                        
                        .pos-name {
                            margin-left: 10px;
                        }
                        
                        > * {
                            vertical-align: middle;
                            display: inline-block;
                        }
                        
                    }
                    
                }
                
                .player-list {
                    height: calc(100vh - 247px);
                    position: relative;
                    padding 15px 0
                    
                    .scrollbar {
                        padding 51px 15px 0
                        height 100%
                    }
                    
                    .head {
                        font-size: 14px;
                        font-weight: bold;
                        height: 36px;
                        position: absolute;
                        top: 15px;
                        left: 15px;
                        right: 15px;
                        z-index 12
                        user-select none
                        background: rgba(255, 255, 255, 0.98)
                        border: 1px solid #E7EDF1;
                        box-shadow: 0 2px 4px 0 rgba(5, 17, 25, 0.06);
                        border-radius: 2px;
                        
                        .col {
                            float: left;
                            height 36px
                            line-height 36px
                            
                            &.player {
                                width: 325px;
                                padding-left: 35px;
                            }
                            
                            &.data {
                                width: 235px;
                                text-align: center;
                            }
                            
                            &.price {
                                width: 110px;
                                text-align: center;
                                cursor: pointer;
                            }
                            
                            span, .icon {
                                display: inline-block;
                                vertical-align: middle;
                                cursor pointer
                            }
                            
                            .icon {
                                width 10px
                                margin-left 8px
                                
                                &.asc {
                                    transform rotate(180deg)
                                }
                                
                                &.desc {
                                    transform rotate(0deg)
                                }
                            }
                        }
                        
                    }
                    
                    .list {
                        .player-item {
                            height: 100px;
                            width: 100%;
                            margin-bottom: 15px;
                            background: #FFFFFF;
                            border: 1px solid #E7EDF1;
