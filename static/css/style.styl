.scrollbar {
    position: relative;
    margin: auto;
    width: 100%;
}

.el-loading-mask {
    z-index 50 !important
    background-color: hsla(0, 0%, 100%, .5);
}

.room > .main .container .left .player-list .list .player-item {
    height: 100px;
    width: 100%;
    margin-bottom: 15px;
    background: #FFFFFF;
    border: 1px solid #E7EDF1;
    box-shadow: 0 2px 4px 0 rgba(226, 234, 239, 0.80);
    border-radius: 2px;
    transition-duration: .3s;
}

.bump {
    -webkit-animation: bump 0.1s ease-out 2 alternate;
    -moz-animation: bump 0.1s ease-out 2 alternate;
    -ms-animation: bump 0.1s ease-out 2 alternate;
    animation: bump 0.1s ease-out 2 alternate;
}

