@font-face {
    font-family: 'TradeGothicLT-Bold';
    src: url('/static/font/Trade-Gothic-LT-Bold.ttf') format('truetype');
}

@font-face {
    font-family: 'TradeGothicLT';
    src: url('/static/font/Trade-Gothic-LT.ttf') format('truetype');
}

@font-face {
    font-family: 'D-DINExp-Bold';
    src: url('/static/font/D-DINExp.ttf') format('truetype');
}

@font-face {
    font-family: 'icon';  /* project id 320800 */
    src: url('//at.alicdn.com/t/font_ab26jko71etmaemi.eot');
    src: url('//at.alicdn.com/t/font_ab26jko71etmaemi.eot?#iefix') format('embedded-opentype'),
            url('//at.alicdn.com/t/font_ab26jko71etmaemi.woff') format('woff'),
            url('//at.alicdn.com/t/font_ab26jko71etmaemi.ttf') format('truetype'),
            url('//at.alicdn.com/t/font_ab26jko71etmaemi.svg#icon') format('svg');
}

* {
    box-sizing: border-box;
    margin: 0;
    padding: 0;
}

input:-webkit-autofill {
    -webkit-box-shadow: 0 0 0px 1000px white inset !important;
}

body {
    background-color: #f3f5f7;
    font-family: "Microsoft YaHei", "TradeGothicLT-Bold", "PingFang SC", 'Source Han Sans CN', sans-serif;
    min-height: 100vh;
    overflow: hidden;
}

.fix-bold-font {
//text-shadow: silver 0px 0px 1px;
}
.flex{
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    &.flex_wrap{
        flex-wrap: wrap;
    }
    &.align_center {
        align-items: center;
    }
    &.justify_between{
        justify-content space-between
    }
    &.justify_end{
        justify-content flex-end
    }
}
.clearfix:after {
    content: ".";
    display: block;
    height: 0;
    clear: both;
    visibility: hidden
}

.container {
    width: 1200px;
    margin: 0 auto;
}

.icon {
    width: 16px;
    height: 16px;
    fill: currentColor;
}

ul,li {
  list-style none
}

a {
    text-decoration: none;
}

a:hover{
    text-decoration: none;
}

body, a {
    color: #373A41;
}

input {
    outline: none;
}

.fl {
    float: left;
}

.fr {
    float: right;
}

.clear {
    clear: both;
}

